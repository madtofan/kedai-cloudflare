{
  "name": "kedai-trpc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "clean": "find . -name node_modules -o -name .vercel -o -name .wrangler -o -name .DS_Store -o -name .next -o -name package-lock.json -o -name .expo -type d -prune | xargs rm -rf",
    "dev": "npm run env-local && next dev --turbo",
    "setup": "npm run clean && npm i && opennextjs-cloudflare && npm run db:migrate:local",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "cross-env NEXT_PUBLIC_LOCAL_DB=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit migrate",
    "db:migrate:local": "npx wrangler d1 execute DB --local --command='SELECT 1' && wrangler d1 migrations apply kedai-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply kedai-db --remote",
    "db:studio": "cross-env NEXT_PUBLIC_LOCAL_DB=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:test": "drizzle-kit studio",
    "env-local": "cp ./.env.dev.local ./.env && cp ./.env.dev.local ./.dev.var",
    "env-deploy": "cp ./.env.deployment ./.env && cp ./.env.deployment ./.dev.vars",
    "env-test": "cp ./.env.test.local ./.env",
    "dev:worker": "npm run env-local && wrangler dev --port 3000",
    "pages:log": "wrangler pages deployment tail --project-name kedai",
    "preview": "npm run env-test && opennextjs-cloudflare && wrangler dev --env test",
    "deploy": "npm run env-deploy && opennextjs-cloudflare && wrangler deploy",
    "tail": "wrangler tail",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@better-fetch/fetch": "^1.1.12",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@react-email/components": "^0.0.32",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "better-auth": "^1.0.10",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "drizzle-orm": "^0.33.0",
    "geist": "^1.3.0",
    "kysely": "^0.27.5",
    "kysely-d1": "^0.3.0",
    "lucide-react": "^0.468.0",
    "nanoid": "^5.0.9",
    "next": "^15.0.1",
    "next-themes": "^0.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "server-only": "^0.0.1",
    "slug": "^10.0.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.5.3",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/slug": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "better-sqlite3": "^11.9.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3",
    "wrangler": "^4.4.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "npm@10.8.2"
}
